v0x6098bf7ae600_0 .net "aresetn", 0 0, v0x6098bf7d6c00_0;  1 drivers
v0x6098bf7d6630_0 .net "clk", 0 0, v0x6098bf7d6cc0_0;  1 drivers
v0x6098bf7d66f0_0 .var "count", 31 0;
