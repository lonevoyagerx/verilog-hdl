v0x6098bf7d6870_0 .net "load", 0 0, v0x6098bf7d6f60_0;  1 drivers
v0x6098bf7d6980_0 .net "loaddata", 31 0, v0x6098bf7d7050_0;  1 drivers
v0x6098bf7d6a60_0 .net "updwn", 0 0, v0x6098bf7d7120_0;  1 drivers
E_0x6098bf7c2000/0 .event negedge, v0x6098bf7ae600_0;
E_0x6098bf7c2000/1 .event posedge, v0x6098bf7d6630_0;
E_0x6098bf7c2000 .event/or E_0x6098bf7c2000/0, E_0x6098bf7c2000/1;
    .scope S_0x6098bf7c12e0;
T_0 ;
    %wait E_0x6098bf7c2000;
    %load/vec4 v0x6098bf7ae600_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0x6098bf7d66f0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x6098bf7d6870_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x6098bf7d6980_0;
    %assign/vec4 v0x6098bf7d66f0_0, 0;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x6098bf7d67b0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.4, 8;
    %load/vec4 v0x6098bf7d6a60_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.6, 8;
    %load/vec4 v0x6098bf7d66f0_0;
    %addi 1, 0, 32;
    %assign/vec4 v0x6098bf7d66f0_0, 0;
    %jmp T_0.7;
T_0.6 ;
    %load/vec4 v0x6098bf7d66f0_0;
    %subi 1, 0, 32;
    %assign/vec4 v0x6098bf7d66f0_0, 0;
T_0.7 ;
T_0.4 ;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x6098bf7c1150;
T_1 ;
    %delay 5, 0;
    %load/vec4 v0x6098bf7d6cc0_0;
    %inv;
    %store/vec4 v0x6098bf7d6cc0_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x6098bf7c1150;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x6098bf7d6cc0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x6098bf7d6c00_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x6098bf7d6f60_0, 0, 1;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x6098bf7d7050_0, 0, 32;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x6098bf7d6e90_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x6098bf7d7120_0, 0, 1;
    %vpi_call 2 31 "$dumpfile", "counter.vcd" {0 0 0};
    %vpi_call 2 32 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x6098bf7c1150 {0 0 0};
    %vpi_call 2 34 "$monitor", "T=%0t | rstn=%b | load=%b | en=%b | updwn=%b | count=%h", $time, v0x6098bf7d6c00_0, v0x6098bf7d6f60_0, v0x6098bf7d6e90_0, v0x6098bf7d7120_0, v0x6098bf7d6d90_0 {0 0 0};
    %delay 3, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x6098bf7d6c00_0, 0, 1;
    %delay 7, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x6098bf7d6c00_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x6098bf7d6f60_0, 0, 1;
    %pushi/vec4 10, 0, 32;
    %store/vec4 v0x6098bf7d7050_0, 0, 32;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x6098bf7d6f60_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x6098bf7d6e90_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x6098bf7d7120_0, 0, 1;
